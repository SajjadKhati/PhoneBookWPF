    <!--  بخاطر ساختار کنترل ShapeTextButton ، به ازای هر یک از شی ای از این نوع کنترل ها ، یک شی Path جدید باید بسازیم .
    
    چون در متد ButtonShape_PropertyChanged در ShapeTextButton مشخص کردیم که هر شی Path ای که اضافه میشود ، به عنوان والد منطقیِ شیِ 
    ShapeTextButton اضافه شود تا بتوانیم در شیِ Path ئه مربوطه ، Binding به ShapeTextButton انجام دهیم .
    و از آنجایی هم که هر کنترل ، فقط یک والد منطقی میپذیرد ، پس یک شی از کنترل Path ، نمیتواند چندین والد یا همان به چندین شی از ShapeTextButton مرتبط باشد .
    -->
    <Path x:Key="AddMobileBevelShape" StrokeThickness="2">
        <Path.Data>
            <RectangleGeometry  RadiusX="6" RadiusY="6">
                <RectangleGeometry.Rect>
                    <MultiBinding Converter="{StaticResource RectConverter}">
                        <Binding RelativeSource="{RelativeSource Mode=FindAncestor, 
                            AncestorType={x:Type customControls:ShapeTextButton}}" Path="ActualWidth"/>
                        <Binding RelativeSource="{RelativeSource Mode=FindAncestor, 
                            AncestorType={x:Type customControls:ShapeTextButton}}" Path="ActualHeight"/>
                    </MultiBinding>
                </RectangleGeometry.Rect>
            </RectangleGeometry>
        </Path.Data>
    </Path>


