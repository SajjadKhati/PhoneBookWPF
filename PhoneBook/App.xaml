<Application x:Class="PhoneBook.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PhoneBook"
             StartupUri="/Window/MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/XamlResource/Style_Template/TemplateResource.xaml"/>
                <ResourceDictionary Source="Resource/XamlResource/Style_Template/DarkStyleResource.xaml"/>

                <ResourceDictionary Source="Resource/XamlResource/IconResource/IconResource.xaml"/>
                <ResourceDictionary Source="Resource/XamlResource/PhoneBookResource/PhoneBookResource.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
